@{
    ViewData["Title"] = "Home Page";
}

@model List<Bowler>

<div class="text-center">
    <h1 class="display-4">Bowler Contact List</h1>
    <table class="table table-bordered table-striped">
        @foreach (Bowler b in Model)
        {
            <tr>
                <td>
                    @b.BowlerFirstName
                </td>
                <td>
                    @b.BowlerLastName
                </td>
            </tr>
        }
    </table>
</div>
